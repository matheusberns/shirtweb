<div class="space">
  <br/>
  <br/>
  <br/>
  <br/>
  <br/>
  <br/>
  <br/>
</div>
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>

  <p class=" text-center">
    <img class="mb-4" src="https://cdn4.iconfinder.com/data/icons/pet-14/64/pawprint-animals-paw-pet-dog-512.png" alt="" width="72" height="72">
  </p>
  <h1 class="h3 mb-3 font-weight-normal text-center">Entrar</h1>
  <div class="field">
    <%= f.label :email, class: "sr-only" %><br/>
    <%= f.email_field :email, autofocus: true, autocomplete: "email", class: "form-control form-signin" %>
  </div>
  <div class="field">
    <%= f.label :password, class: "sr-only" %><br/>
    <%= f.password_field :password, autocomplete: "current-password", class: "form-control form-signin" %>
  </div>

  <div class="field">
    <% if devise_mapping.rememberable? %>
      <div class="flex field checkbox mb-3 text-right form-signin">
        <%= f.check_box :remember_me %>
        <%= f.label :remember_me %>
      </div>
    <% end %>
  </div>
  <div class="actions">
    <%= f.submit "Entrar", class: "btn btn-lg btn-primary btn-block form-signin" %>
  </div>
<% end %>
<form class="form-signin text-center">
  <%= render "devise/shared/links" %>
  <p class="mt-5 mb-3 text-muted">&copy; 2019</p>
</form>






